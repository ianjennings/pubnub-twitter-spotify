      script(src='http://cdn.embed.ly/jquery.embedly-3.1.1.min.js', type='text/javascript')
    title Popular Spotify Tracks Posted On Twitter
  body
    script.
      
      $.embedly.defaults.key = '5f535050d3f9484c94cd07ea3121299b';

      $.get('/top', function(data) {
        
        var urls = [];
        for(var i = 0; i < data.length; i++) {
          urls.push(data[i].link);
        }

        console.log(urls);

        $.embedly.extract(urls.map(escape).join(','), {key: 'Your Embedly Key'}).progress(function(data){
          console.log(data);
        });

      });

